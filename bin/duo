#!/usr/bin/env node

/**
 * Module dependencies.
 */

var path = require('path');
var spawn = require('win-fork');

/**
 * Resolve.
 */

var duo = path.resolve(__dirname, '_duo');
var args = process.argv.slice(2);

/**
 * Spawn.
 */

var proc = spawn(duo, args, { stdio: 'inherit' });

/**
 * Exit.
 */

proc.on('exit', function (code) {
  process.exit(code);
});
